{
    "manifest_version": 2,

    "name": "Github Issue ",
    "version": "0.1.0",
    "description": "Close Issue",
    "permissions": ["tabs", "storage"],
    "content_scripts": [{
        "css": ["styles.css"],
        "js": ["content.js"],
        "matches": ["https://github.com/issues*", "https://github.com/issues/*", "https://github.com/*/*/issues**"]
    }],
    "background": {
        "scripts": ["background.js"]
    }

}